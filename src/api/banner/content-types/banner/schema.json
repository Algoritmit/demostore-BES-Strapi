{
  "kind": "collectionType",
  "collectionName": "banners",
  "info": {
    "singularName": "banner",
    "pluralName": "banners",
    "displayName": "Banner",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "subtitle": {
      "type": "text"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "groups": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::group.group"
    },
    "href": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "start": {
      "type": "date",
      "required": true
    },
    "finish": {
      "type": "date",
      "required": true
    },
    "className": {
      "type": "string"
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "AD",
        "DISCOUNT",
        "NEWS"
      ],
      "default": "NEWS"
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "label": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    }
  }
}
