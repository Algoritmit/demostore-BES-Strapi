{
  "kind": "collectionType",
  "collectionName": "showcases",
  "info": {
    "singularName": "showcase",
    "pluralName": "showcases",
    "displayName": "showcase",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article"
    },
    "groups": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::group.group"
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
