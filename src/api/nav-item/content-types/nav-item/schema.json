{
  "kind": "collectionType",
  "collectionName": "nav_items",
  "info": {
    "singularName": "nav-item",
    "pluralName": "nav-items",
    "displayName": "NavItem",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "subtitle": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "href": {
      "type": "string"
    },
    "className": {
      "type": "string"
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "header-left",
        "header-centre",
        "header-right",
        "footer-left",
        "footer-centre",
        "footer-right",
        "mobile-only"
      ]
    },
    "auth": {
      "type": "boolean",
      "default": false
    },
    "group": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::group.group"
    },
    "article": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::article.article"
    },
    "imageOnly": {
      "type": "boolean",
      "default": false
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "externalLink": {
      "type": "boolean",
      "default": false
    }
  }
}
